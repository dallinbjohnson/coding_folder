<div class="pbody">
	<header>
		
		<ul>
			<li><a href="https://www.pinterest.com/" target="_blank"><%= image_tag("pin_img/Pinterest-logo.png", :alt => "Pinterest Logo", :class=> "logo") %></a></li>
			<li><input type="text" name="search" placeholder="Search" class="search"></li>
			<li class="icon"><a href="#"><i class="fa fa-comment" aria-hidden="true"></i></a></li>
			<li class="icon"><a href="#"><i class="fa fa-user" aria-hidden="true" ></i></a></li>
			<li class="icon"><a href="#"><i class="fa fa-bars" aria-hidden="true" ></i></a></li>
		</ul>

	</header>

	<content>
		
		<div class="colume">

			<div class="post">
				<%= image_tag("pin_img/IMG_0009.jpg", :alt => "") %>
